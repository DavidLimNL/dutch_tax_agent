# Dutch Tax Agent - Environment Variables
# Copy this file to .env and fill in your values

# ============================================================================
# OpenAI Configuration (Required)
# ============================================================================
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini

# ============================================================================
# LangSmith Configuration (Optional but Recommended)
# ============================================================================
# Get your API key from: https://smith.langchain.com (or EU endpoint)
# Enable tracing to see detailed execution logs in LangSmith
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=dutch-tax-agent
LANGSMITH_TRACING=true

# For EU region users, uncomment and set your EU endpoint:
# LANGSMITH_ENDPOINT=https://eu.smith.langchain.com

# ============================================================================
# ECB (European Central Bank) Configuration (Optional)
# ============================================================================
# For currency conversion. If not provided, will use cached rates.
# ECB_API_KEY=your_ecb_api_key_here
ECB_RATE_CACHE_DAYS=7

# ============================================================================
# Application Configuration
# ============================================================================
LOG_LEVEL=INFO
MAX_DOCUMENT_SIZE_MB=10
PDF_MIN_CHARS=50
SUPPORTED_TAX_YEARS=2022,2023,2024,2025

# ============================================================================
# Document Processing
# ============================================================================
ENABLE_PARALLEL_PARSING=true
MAX_PARALLEL_DOCS=10
